<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<parameters>
		<parameter key="sylius_sandbox_installer.setup.class">Sylius\Sandbox\Bundle\InstallerBundle\Setup\Setup</parameter>
	</parameters>

    <services>
        <service id="sylius_sandbox_installer.setup" class="%sylius_sandbox_installer.setup.class%">
        	<tag name="sylius_installer.setup" alias="sandbox-install" />
        </service>
    </services>
    
</container>
